{
  "expo": {
    "name": "SBSMobile",
    "slug": "SBSMobile",
    "scheme": "SBSMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sbsicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/sbsicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sbsicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mirack.SBSMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-router",
      [
        "expo-dev-client",
        {
          "developmentClient": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/sbsicon.png",
          "dark": {
            "image": "./assets/sbsicon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c2b5ba9b-0466-473e-b4b7-d250e6cfddda"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c2b5ba9b-0466-473e-b4b7-d250e6cfddda"
    }
  }
}
